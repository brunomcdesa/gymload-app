{
  "name": "Gymload",
  "displayName": "Gymload",
  "expo": {
    "name": "Gymload",
    "slug": "gymloadapp",
    "platforms": ["ios", "android", "web"],
    "newArchEnabled": false,
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.gymloadapp"
    },
    "android": {
      "package": "com.gymloadapp",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE"
      ]
    },
    "plugins": [
      "expo-secure-store",
      [
        "react-native-google-mobile-ads",
        {
          "android_app_id": "ca-app-pub-3977686093704418~7718286453",
          "ios_app_id": "ca-app-pub-xxxxxxxx~xxxxxxxx",
          "androidAppId": "ca-app-pub-3977686093704418~7718286453",
          "iosAppId": "ca-app-pub-xxxxxxxx~xxxxxxxx"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "O aplicativo precisa acessar suas fotos para permitir que você escolha uma imagem de perfil.",
          "cameraPermission": "O aplicativo precisa acessar sua câmera para permitir que você tire uma foto para o perfil."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "4cc1a22d-3e62-4dc1-becb-c6a0cb559beb"
      }
    },
    "owner": "brunomcdesa"
  }
}
